    <cxf:bus>
        <cxf:properties>
            <!--entry key="ws-security.global.ehcachemanager"
                   value-ref="cacheManagerFactory"/>
            <entry>
                <key><util:constant static-field="org.apache.cxf.ws.security.SecurityConstants.CACHE_CONFIG_FILE"/></key>
                <value>classpath*:cxf-ehcache.xml</value>
            </entry-->

            <entry>
                <key>
                    <util:constant static-field="org.apache.cxf.ws.security.SecurityConstants.ENABLE_NONCE_CACHE"/>
                </key>
                <value>false</value>
            </entry>
        </cxf:properties>
        <cxf:inInterceptors>
            <!--bean id="loggingInInterceptor"
                  class="com.viettel.fw.logging.LoggingInInterceptor">
                <property name="prettyLogging" value="false"/>
            </bean-->

            <bean id="wss4JInInterceptor" class="org.apache.cxf.ws.security.wss4j.WSS4JInInterceptor">
                <constructor-arg>
                    <map>
                        <entry key="action" value="UsernameToken"/>
                        <entry key="passwordType" value="PasswordText"/>
                        <entry key="passwordCallbackRef" value-ref="cxfWsClientFactory"/>
                    </map>
                </constructor-arg>
            </bean>
        </cxf:inInterceptors>
        <cxf:outInterceptors>
            <bean class="org.apache.cxf.transport.common.gzip.GZIPOutInterceptor"/>
            <!--bean id="loggingOutInterceptor" class="com.viettel.fw.logging.LoggingOutInterceptor">
                <property name="prettyLogging" value="false"/>
            </bean-->
        </cxf:outInterceptors>

    </cxf:bus>

